{
    "name": "zizther/replace-variable",
    "description": "Replace general or config variables, and global fields",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "replace variable"
    ],
    "support": {
        "docs": "https://github.com/zizther/replace-variable/blob/master/README.md",
        "issues": "https://github.com/zizther/replace-variable/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Reed",
            "homepage": "https://vimia.co.uk"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "require-dev": {
      "craftcms/cms": "^3.2.0",
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "zizther\\replacevariable\\": "src/"
        }
    },
    "extra": {
        "name": "Replace Variable",
        "handle": "replace-variable",
        "developer": "Nathan Reed",
        "developerUrl": "https://vimia.co.uk",
        "documentationUrl": "https://github.com/zizther/replace-variable/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/zizther/replace-variable/master/CHANGELOG.md",
        "components": {
            "replaceVariableService": "zizther\\replacevariable\\services\\ReplaceVariableService"
        },
        "class": "zizther\\replacevariable\\ReplaceVariable"
    }
}
